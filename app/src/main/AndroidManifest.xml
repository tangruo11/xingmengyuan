        android:supportsRtl="true"
        android:theme="@style/statusbar"
        android:usesCleartextTraffic="true">
        <activity android:name=".home.GuideActivity"></activity>
        <activity android:name=".home.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".luckFrg.LuckAnalysisActivity" />
        <activity android:name=".partnerFrg.PartnerAnalysActivity" />
        <activity android:name=".starFrag.StarAnalysisActivity" />
        <activity android:name=".Mainactivity">    </activity>
    </application>

</manifest>